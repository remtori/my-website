{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "module": "esnext",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "moduleResolution": "node",
    "composite": true,
    "declarationMap": true,
    "sourceMap": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "skipLibCheck": true,
    "types": [
    ],
    "baseUrl": "./"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}